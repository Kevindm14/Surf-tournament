<h2>Participants</h2>
<table>
  <tr>
    <th>Name<th>
    <th>Last name</th>
    <th>Gender</th>
    <th>Categories</th>
  </tr>

  <% @participants.each do |participant| %>
    <tr>
      <td><%= participant.name  %><td>
      <td><%= participant.last_name %></td>
      <td><%= participant.gender %></td>
      <% participant.categories.each do |category| %>
      <td><%= category.name %></td>
      <% end %>
      <td><%= link_to 'Edit', edit_participant_path(participant) %></td>
      <td><%= link_to 'Delete', participant, method: :delete, data: { confirm: 'Â¿Are you sure?'} %></td>
    </tr>
  <% end %>
      <%= link_to 'Add participant', new_participant_path %>
</table>
